{
  "name": "workos-convex-sveltekit",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "version": "1.0.1",
  "description": "WorkOS + Convex authentication for SvelteKit",
  "files": [
    "dist",
    "templates"
  ],
  "bin": {
    "workos-convex-sveltekit": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schema": {
      "import": "./dist/schema.js",
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slimnate/workos-convex-sveltekit.git"
  },
  "keywords": [
    "workos",
    "authkit",
    "auth",
    "convex",
    "convexdb",
    "svelte",
    "sveltekit"
  ],
  "author": "Nathan Hoyt <slimnatekc@gmail.com> (https:/slimnate.net/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/slimnate/workos-convex-sveltekit/issues"
  },
  "homepage": "https://github.com/slimnate/workos-convex-sveltekit#readme",
  "devDependencies": {
    "@types/diff": "^7.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.9.1",
    "convex": "^1.28.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "diff": "^5.2.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "picocolors": "^1.0.0",
    "convex": "^1.28.0"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.48.0",
    "@workos/authkit-sveltekit": "^0.0.1-alpha.0"
  }
}
